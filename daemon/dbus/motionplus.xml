<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
	<interface name="org.wiimotedev.motionplus">
		<method name="isMotionPlusSupported">
			<arg name="value" type="b" direction="out" />
		</method>
		<method name="isMotionPlusEnabled">
			<arg name="id" type="u" direction="in" />
			<arg name="value" type="b" direction="out" />
		</method>
		<method name="setMotionPlusEnabled">
			<arg name="id" type="u" direction="in" />
			<arg name="value" type="b" direction="out" />
		</method>
		<signal name="motionPlusDataChanged">
			<arg name="id" type="u" direction="out" />
			<arg name="x" type="i" direction="out" />
			<arg name="y" type="i" direction="out" />
			<arg name="z" type="i" direction="out" />
			<arg name="lowX" type="i" direction="out" />
			<arg name="lowY" type="i" direction="out" />
			<arg name="lowZ" type="i" direction="out" />
		</signal>
	</interface>
</node>
