<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
	<interface name="org.wiimotedev.virtualcursor">
		<method name="list">
				<annotation value="QList&lt;uint>" name="org.qtproject.QtDBus.QtTypeName.Out0" />
				<arg direction="out" type="au" name="list" />
		</method>
		<signal name="visibilityChanged">
			<arg name="id" type="u" direction="out" />
			<arg name="visible" type="b" direction="out" />
		</signal>
		<signal name="dataChanged">
			<arg name="id" type="u" direction="out" />
			<arg name="x" type="d" direction="out" />
			<arg name="y" type="d" direction="out" />
			<arg name="yaw" type="d" direction="out" />
			<arg name="roll" type="d" direction="out" />
			<arg name="pitch" type="d" direction="out" />
			<arg name="distance_cm" type="d" direction="out" />
			<arg name="visible" type="b" direction="out" />
		</signal>
	</interface>
</node>
